		TextElement.FontWeight="Regular"
		TextElement.FontSize="16"
		TextOptions.TextFormattingMode="Ideal"
		TextOptions.TextRenderingMode="Auto"
        
		Background= "{DynamicResource MaterialDesignPaper}"
		FontFamily= "{DynamicResource MaterialDesignFont}" Loaded="StartUpProgramLoaded" Unloaded="CloseProgramUnloaded"
        Title="OutputControl" MinWidth="700" MinHeight="320" Height="930" Width="1320" Icon="/Resource/logo_icon.ico" >

