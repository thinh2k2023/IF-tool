<Window x:Class="WPFiftool.Views.UserManualWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Style="{StaticResource MaterialDesignWindow}" 
        Title="User Manual Simulation IF tool " Height="600" Width="800" Icon="/Resource/logo_icon.ico" MinHeight="200" MinWidth="300">
    <Window.Resources>
        <ResourceDictionary Source="/Resource/StyleResource.xaml"></ResourceDictionary>
    </Window.Resources>

    <Grid Margin="10,0,10,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>
