                        <Border Style="{StaticResource border}" x:Name="Border_AC3" MouseRightButtonUp="AC3_MouseRightButtonUp"  >
                            <Canvas Style="{StaticResource canvasAC}">
                                <TextBlock Name="AC3_Name" Style="{StaticResource TextBlock_ComponentName}" Text="AC OUT 3"></TextBlock>

                                <Rectangle Fill="White" Height="35" Width="130" Margin="10,25" RadiusY="12" RadiusX="12" />
                                <TextBox Name="AC3_Rms" Text="0" Width="70" Height="35" Margin="40,25" VerticalAlignment="Center" Background="White" 
                                         BorderBrush="{x:Null}" FontSize="13" TextAlignment="Center" Padding="0,0,0,0" PreviewTextInput="Duty_RMS_Textbox_PreviewTextInput"
                                         materialDesign:HintAssist.Hint="Voltage Rms" materialDesign:TextFieldAssist.SuffixText ="V"
                                         Style="{StaticResource MaterialDesignFloatingHintTextBox}" LostFocus="AC3_RMS_lostFocus" KeyDown="AC3_RMS_KeyDown" />
                                <Button Content="-" Name="AC3_Rms_minus" Height="35" Width="30" Margin="10,25" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_RMS_minus_Click" />
                                <Button Content="+" Name="AC3_Rms_plus" Height="35" Width="30" Margin="110,25" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_RMS_plus_Click" />

                                <Rectangle Fill="White" Height="35" Width="130" Margin="150,25" RadiusY="12" RadiusX="12" />
                                <TextBox Name="AC3_Freq" Text="0" Width="70" Height="35" Margin="180,25" VerticalAlignment="Center" Background="White" 
                                         BorderBrush="{x:Null}" FontSize="13" TextAlignment="Center" Padding="0,0,0,0" PreviewTextInput="Freq_Textbox_PreviewTextInput"
                                         materialDesign:HintAssist.Hint="Frequency" materialDesign:TextFieldAssist.SuffixText ="Hz"
                                         Style="{StaticResource MaterialDesignFloatingHintTextBox}" LostFocus="AC3_Freq_lostFocus" KeyDown="AC3_Freq_KeyDown" />
                                <Button Content="-" Name="AC3_Freq_minus" Height="35" Width="30" Margin="150,25" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_Freq_minus_Click" />
                                <Button Content="+" Name="AC3_Freq_plus" Height="35" Width="30" Margin="250,25" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_Freq_plus_Click" />

                                <Rectangle Fill="White" Height="35" Width="130" Margin="10,80" RadiusY="12" RadiusX="12" />
                                <TextBox Name="AC3_PhaseShift" Text="0" Width="70" Height="35" Margin="40,80" VerticalAlignment="Center" Background="White" 
                                         BorderBrush="{x:Null}" FontSize="13" TextAlignment="Center" Padding="0,0,0,0" PreviewTextInput="PS_Textbox_PreviewTextInput"
                                         materialDesign:HintAssist.Hint="Phase shift" materialDesign:TextFieldAssist.SuffixText ="°"
                                         Style="{StaticResource MaterialDesignFloatingHintTextBox}" LostFocus="AC3_PhaseShift_lostFocus" KeyDown="AC3_PhaseShift_KeyDown" />
                                <Button Content="-" Name="AC3_PhaseShift_minus" Height="35" Width="30" Margin="10,80" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_PhaseShift_minus_Click" />
                                <Button Content="+" Name="AC3_PhaseShift_plus" Height="35" Width="30" Margin="110,80" Foreground="#FF1D2FC5" Background="{x:Null}" 
                                        FontSize="20" FontFamily="Segoe UI" Padding="0,-5,0,0" BorderBrush="{x:Null}" FontWeight="Bold" 
                                        Style="{StaticResource MaterialDesignOutlinedButton}" Click="AC3_PhaseShift_plus_Click" />

                                <ComboBox x:Name="AC3_TypeWave" Style="{StaticResource MaterialDesignOutlinedComboBox}"
                                    Margin="150,80" Width="130" Height="35"
                                    materialDesign:HintAssist.Hint="AC type wave" SelectedIndex="0" 
                                    HorizontalAlignment="Left" Padding="3,3,3,3" SelectionChanged="AC3_TypeWave_SelectChange" >
                                    <ComboBoxItem Content="Sine-Wave"/>
                                    <ComboBoxItem Content="Full-Wave"/>
                                    <ComboBoxItem Content="Half-Wave"/>
                                </ComboBox>
                            </Canvas>
                        </Border>

                    </WrapPanel>
                </WrapPanel>
            </ScrollViewer>

        </Grid>
        <Grid VerticalAlignment="Bottom" >
            <Canvas Height="60" >
                <Button
                    Width="90" Height="35"
                    Content="Apply" Canvas.Right = "40" Canvas.Top ="10" Click="Apply_Button_Click" 
                    IsEnabled="{Binding DataContext.ControlsEnabled, RelativeSource={RelativeSource FindAncestor, AncestorType=Window}}"
                    Style="{StaticResource MaterialDesignPaperLightButton}"
                    ToolTip="Apply to save configuration" BorderBrush="#FFC9E1F3" />
            </Canvas>
        </Grid>
    </Grid>
</Window>
