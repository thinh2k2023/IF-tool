        <!--declare some variables-->
        <system:Double x:Key="AC_Out0_rms">20</system:Double>

        <ContextMenu x:Key="ComponentContextMenu" Padding="0,0,0,0" >
            <MenuItem Header="Remove" Click="Remove_Component_MouseLeftButtonUp" />
            <!--<Separator/>-->
        </ContextMenu>
    </Window.Resources>

